
config global
	option anon_mount '0'
	option auto_swap '1'
	option auto_mount '1'
	option delay_root '5'
	option check_fs '0'
	option anon_swap '1'

config mount
	option target '/mnt/sda2'
	option fstype 'ext3'
	option enabled '1'
	option device '/dev/sda2'

config swap
	option device '/dev/sda1'
	option enabled '1'

